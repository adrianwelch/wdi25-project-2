<div class="container">
  <form method="POST" action="/dives" enctype="multipart/form-data" name="_method">
    <div class="form-group">
      <label for="diveNo">Dive No.</label>
      <input class="form-control" type="number" name="diveNo" id="diveNo">
    </div>
    <div class="form-group">
      <label for="date">Date</label>
      <input class="form-control" type="date" name="date" id="date">
    </div>
    <div class="form-group">
      <label for="diveTime"> Dive Time (mins)</label>
      <input class="form-control" type="number" name="diveTime" id="diveTime">
    </div>
    <div class="form-group">
      <label for="diveShop">Dive Shop</label>
      <input class="form-control" type="text" name="diveShop" id="diveShop" placeholder="
      ">
    </div>
    <div class="form-group">
      <label for="location">Location</label>
      <input class="autocomplete form-control" id="location" name="location" placeholder="Enter Location" style="width: 100%">
      <input type="hidden" name="latitude">
      <input type="hidden" name="longitude">
    </div>
    <input type="hidden" name="lat" lat="">
    <input type="hidden" name="lng" long="">
    <div class="form-group">
    <label for="diveSite">Dive Site</label>
    <input class="form-control" name="diveSite">
    </div>
    <div class="form-group">
      <label for="animalTags">Animal Tags</label>
      <input class="form-control" type="text" name="animalTags" id="animalTags">
    </div>
    <div class="form-group">
      <label for="stars">Stars</label>
      <input class="form-control" type="number" name="stars" id="stars" min="1" max="5">
    </div>
    <%# <div class="form-group">
      <label for="review">Review</label>
      <input type= "text" textarea class="form-control" class="form-control" name="review id="review" rows="3"></textarea>
    </div>%>
    <div>
     <label for="review">Review</label>
     <input type="text" class="form-control" rows="3" name="review" id="review">
   </div>
    <div class="form-group">
      <label for="image">Image</label>
      <input class="form-control" type="file" name="image" id="image">
    </div>

    <button class="btn btn-primary">Post</button>
  </form>
</div>
